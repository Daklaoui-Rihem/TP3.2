package compte;

public class personne {
	private String nom;
	private String prenom;
	private String adresse;
	public personne(String n, String p, String a)
	{
		this.nom=n;
		this.prenom=p;
		this.adresse=a;
	}
	public String toString()
	{
		return "Le nom est:"+n+" Le prenom est:"+p+" l adresse est:"+a;
	}
}
