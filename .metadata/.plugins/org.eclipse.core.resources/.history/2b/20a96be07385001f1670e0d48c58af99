package compte;

public class personne {
	private String nom;
	private String prenom;
	private String adresse;
	public personne(String n, String p, String a)
	{
		this.nom=n;
		this.prenom=p;
		this.adresse=a;
	}
	public String toString()
	{
		return "Le nom est:"+nom+" Le prenom est:"+prenom+" l adresse est:"+adresse;
	}
	
}
